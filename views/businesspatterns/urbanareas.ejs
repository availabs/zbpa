<style>
	body{
		margin-top: 30px;
	}
	.nv-line{
		fill:none;
	}
    body {
      width: 960px;
      margin: auto;
      position: relative;
    }

    svg {
      font: 11px "Helvetica Neue", Helvetica, Arial, sans-serif;
    }

    .axis path,
    .axis line {
      fill: none;
      stroke: #000;
      shape-rendering: crispEdges;
    }

    /*.axis--y path {
      display: none;
    }*/

    .urbanAreas {
      fill: none;
      stroke: #aaa;
      stroke-linejoin: round;
      stroke-linecap: round;
      stroke-width: 1.5px;
    }

    .urbanAreas--hover {
      stroke: #000;
    }

    .focus text {
      text-anchor: middle;
      text-shadow: 0 1px 0 #fff, 1px 0 0 #fff, 0 -1px 0 #fff, -1px 0 0 #fff;
    }

    .voronoi path {
      fill: none;
      pointer-events: all;
    }

    .voronoi--show path {
      stroke: red;
      stroke-opacity: .2;
    }

    #form {
      position: absolute;
      top: 20px;
      right: 30px;
    }
</style>
